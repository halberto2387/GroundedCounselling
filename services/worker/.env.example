# Environment Variables for Worker Service

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/grounded_counselling

# Redis
REDIS_URL=redis://localhost:6379/0

# Email (Resend)
RESEND_API_KEY=your-resend-api-key

# SMS (Twilio)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# File Storage (AWS S3)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_BUCKET_NAME=grounded-counselling-uploads

# Calendar Integration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
OUTLOOK_CLIENT_ID=your-outlook-client-id
OUTLOOK_CLIENT_SECRET=your-outlook-client-secret

# Search (Meilisearch)
MEILISEARCH_URL=http://localhost:7700
MEILISEARCH_API_KEY=your-meilisearch-api-key

# Encryption Key for PHI (Personal Health Information)
PHI_ENCRYPTION_KEY=your-32-character-encryption-key