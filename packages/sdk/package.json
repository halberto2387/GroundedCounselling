{
  "name": "@grounded-counselling/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for GroundedCounselling API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate": "node scripts/generate.js"
  },
  "devDependencies": {
    "@grounded-counselling/config": "workspace:*",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "openapi-typescript": "^6.7.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}